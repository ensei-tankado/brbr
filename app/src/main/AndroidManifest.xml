<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brbr">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/people"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".BarberRoute"/>
        <activity android:name=".CustomerRoute"/>
        <activity android:name=".BarberRegister"/>
        <activity android:name=".CustomerRegister"/>
        <activity android:name=".BarberLogin"/>
        <activity android:name=".CustomerLogin"/>
        <activity android:name=".BarberHome"/>
        <activity android:name=".CustomerHome"/>
        <activity android:name=".BookRequest"/>
        <activity android:name=".RequestDetails"/>
        <activity android:name=".BookingHistory"/>
        <activity android:name=".BookingHistoryDetail"/>
        <activity android:name=".ConfirmedBooking"/>
        <activity android:name=".ConfirmedBookingDetails"/>
    </application>

</manifest>